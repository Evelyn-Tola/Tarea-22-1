<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="355.0" prefWidth="346.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.javafxestudiantes.PrimaryController">
   <children>
      <Label text="ESTUDIANTES" />
      <TableView fx:id="tblEstudiantes" prefHeight="168.0" prefWidth="238.0">
        <columns>
          <TableColumn fx:id="colNombre" prefWidth="105.0" text="Nombre" />
          <TableColumn fx:id="colApellido" prefWidth="98.0" text="Apellido" />
            <TableColumn fx:id="colEdad" prefWidth="101.0" text="Edad" />
        </columns>
      </TableView>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="CARGAR" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
